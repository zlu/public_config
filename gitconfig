[user]
	name = Zhao Lu
	email = zlu@me.com
[include]
	path = $HOME/.github_config

[tokenuser]
	name = Zhao Lu
	email = zlu@me.com
[core]
	editor = emacs
	excludesfile = ~/.gitignore_global
	url=git@github.com:zlu

[color]
	diff = auto
	branch = auto
	status = auto

[alias]
	a  = add
	b  = branch
	ba = branch -a 
	ci =  commit -v
	d  = diff -b
	co = checkout
	s  = status

[apply]
	whitespace = nowarn

[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 

[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
